{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"arrowParens": "avoid",
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"tabWidth": 2
}
